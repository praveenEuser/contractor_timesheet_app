      "source": "^/admin_dashboard/webapp/(.*)$",
      "target": "$1",
      "localDir": "admin_dashboard/webapp/",
      "authenticationType": "xsuaa",
      "csrfProtection": false,
      "scope": ["$XSAPPNAME.AdminRole"]
    },
    {
      "source": "^/admin_pages/managers/webapp/(.*)$",
      "target": "$1",
      "localDir": "admin_pages/managers/webapp/",
      "authenticationType": "xsuaa",
